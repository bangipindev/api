[mysqld]
user                = mysql
socket              = /var/run/mysqld/mysqld.sock
port                = 3306
basedir             = /usr
datadir             = /var/lib/mysql
tmpdir              = /tmp
#bind-address        = 0.0.0.0
thread_stack        = 192K
thread_cache_size   = 8
query_cache_limit   = 1M
query_cache_size    = 16M
expire_logs_days    = 10
max_binlog_size     = 100M
default-time-zone   = "+07:00"

[client]
#host                = 192.168.100.10
#port                = 33066
socket              = /var/run/mysqld/mysqld.sock

[mysqld_safe]
socket              = /var/run/mysqld/mysqld.sock
nice                = 0

[mysqldump]
quick
quote-names
max_allowed_packet  = 16M

[mysql]

[isamchk]

#skip-networking
